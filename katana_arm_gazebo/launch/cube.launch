<?xml version="1.0"?>
<launch>
  <!-- send cube.urdf to param server -->
  <param name="object_description1" textfile="$(find katana_arm_gazebo)/urdf/coke_can.urdf" />

  <!-- push object_description to factory and spawn in gazebo -->
  <node name="spawn_object1" pkg="gazebo_ros" type="spawn_model" args="-urdf -param object_description1 -x 0.60 -y 0.205 -z 0.30 -model _obj_CUBE-ORANGE" respawn="false" output="screen" />
</launch>



